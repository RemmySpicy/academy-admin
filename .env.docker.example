# Docker Development Environment Variables Example
# Copy this file to .env.docker and modify the values

# Database Configuration
POSTGRES_DB=academy_admin
POSTGRES_USER=admin
POSTGRES_PASSWORD=your_secure_password_here

# Backend Configuration
DATABASE_URL=postgresql://admin:your_secure_password_here@db:5432/academy_admin
DEBUG=true
SECRET_KEY=your-secret-key-for-development-only-change-in-production
CORS_ORIGINS=http://localhost:3000,http://frontend:3000

# Frontend Configuration
NEXT_PUBLIC_API_URL=http://localhost:8000